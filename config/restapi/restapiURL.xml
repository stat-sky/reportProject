<?xml version="1.0" encoding="UTF-8"?>
<configurations>
	<configuration>
		<purpose>用户登录</purpose>
		<url>/users/login</url>
		<http-method>post</http-method>
	</configuration>
	<configuration>
		<purpose>用户登出</purpose>
		<url>/users/logout</url>
		<http-method>get</http-method>
	</configuration>
	<configuration>
		<purpose>查询服务</purpose>
		<url>/services/{serviceId}[?view={viewtype}]</url>
		<http-method>get</http-method>
	</configuration>
	<configuration>
		<purpose>查询多个服务</purpose>
		<url>/services[?clusterId={clusterId}&amp;view={viewtype}]</url>
		<http-method>get</http-method>
	</configuration>
	<configuration>
		<purpose>查询服务角色</purpose>
		<url>/serviceRoles/{serviceRoleId}</url>
		<http-method>get</http-method>
	</configuration>
	<configuration>
		<purpose>查询多个服务角色</purpose>
		<url>/serviceRoles[?serviceId={serviceId}&amp;roleType={serviceRoleType}]</url>
		<http-method>get</http-method>
	</configuration>
	<configuration>
		<purpose>下载服务配置</purpose>
		<url>/services/{serviceId}/exportConfig?fileName={fileName}</url>
		<http-method>get</http-method>
	</configuration>
	<configuration>
		<purpose>下载用户keytab</purpose>
		<url>/users/{userType}/{username}/keytab/{filename}</url>
		<http-method>get</http-method>
	</configuration>
	<configuration>
		<purpose>查询节点</purpose>
		<url>/nodes/{nodeId}[?view={viewtype}]</url>
		<http-method>get</http-method>
	</configuration>
	<configuration>
		<purpose>查询多个节点</purpose>
		<url>/nodes[?view={viewtype}]</url>
		<http-method>get</http-method>
	</configuration>
	<configuration>
		<purpose>集群指标查询</purpose>
		<url>/clusters/{clusterId}/metric?metricnames={metricsName}&amp;start={startTimeStamp}&amp;end={endTimeStamp}</url>
		<http-method>get</http-method>
	</configuration>
	<configuration>
		<purpose>服务指标查询</purpose>
		<url>/services/{serviceId}/metric?metricnames={metricsName}&amp;start={startTimeStamp}&amp;end={endTimeStamp}</url>
		<http-method>get</http-method>
	</configuration>
	<configuration>
		<purpose>节点指标查询</purpose>
		<url>/nodes/{nodeId}/metric?metricnames={metricsName}&amp;start={startTimeStamp}&amp;end={endTimeStamp}</url>
		<http-method>get</http-method>
	</configuration>
	<configuration>
		<purpose>服务角色指标查询</purpose>
		<url>/roles/{roleId}/metric?metricnames={metricsName}&amp;start={startTimeStamp}&amp;end={endTimeStamp}</url>
		<http-method>get</http-method>
	</configuration>
</configurations>